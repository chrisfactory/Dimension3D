    <local:CenterSizeConverter x:Key="CenterSizeConverter"/>

    <MaterialGroup  x:Key="faceMaterial">
        <DiffuseMaterial>
            <DiffuseMaterial.Brush>
                <SolidColorBrush Color="#FF48BDD8" Opacity="0.4"/>
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="64">
            <SpecularMaterial.Brush>
                <SolidColorBrush Color="#FF48BDD8" Opacity="0.7"/>
            </SpecularMaterial.Brush>
        </SpecularMaterial>
    </MaterialGroup>


    <MaterialGroup  x:Key="edgeMaterial">
        <DiffuseMaterial>
            <DiffuseMaterial.Brush>
                <SolidColorBrush Color="#FFCB44CD" Opacity="0.4"/>
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="8">
            <SpecularMaterial.Brush>
                <SolidColorBrush Color="#FFCB44CD" Opacity="0.7"/>
            </SpecularMaterial.Brush>
        </SpecularMaterial>
    </MaterialGroup>

    <MaterialGroup  x:Key="vertexMaterial">
        <DiffuseMaterial>
            <DiffuseMaterial.Brush>
                <SolidColorBrush Color="#FF7CD146" Opacity="0.4"/>
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="8">
            <SpecularMaterial.Brush>
                <SolidColorBrush Color="#FF7CD146" Opacity="0.7"/>
            </SpecularMaterial.Brush>
        </SpecularMaterial>
    </MaterialGroup>

    <MaterialGroup  x:Key="mouseOverMaterial">
        <DiffuseMaterial>
            <DiffuseMaterial.Brush>
                <SolidColorBrush Color="#FFFFB555" Opacity="0.1"/>
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="8">
            <SpecularMaterial.Brush>
                <SolidColorBrush Color="#FFFFB555" Opacity="0.7"/>
            </SpecularMaterial.Brush>
        </SpecularMaterial>
    </MaterialGroup>

    <MaterialGroup  x:Key="clickMaterial">
        <DiffuseMaterial>
            <DiffuseMaterial.Brush>
                <SolidColorBrush Color="#FFCC7300" Opacity="0.1"/>
            </DiffuseMaterial.Brush>
        </DiffuseMaterial>
        <SpecularMaterial SpecularPower="8">
            <SpecularMaterial.Brush>
                <SolidColorBrush Color="#FFCC7300" Opacity="0.7"/>
            </SpecularMaterial.Brush>
        </SpecularMaterial>
    </MaterialGroup> 

    <Style x:Key="FaceButtonStyle" TargetType="{x:Type local:DimensionButton3D}">
        <Setter Property="Template" >
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:DimensionButton3D}">
                    <Grid>
                        <ItemsPresenter/>
                        <local:Model3DPresenter x:Name="PART_MODEL" >
                            <Model3DGroup>
                                <GeometryModel3D x:Name="g" Material="{StaticResource faceMaterial}" BackMaterial="{StaticResource faceMaterial}">
                                    <local:GeometryProvider.Provider>
                                        <local:DimensionGeometryOctagon Side="Front" Location="0,0,0" Size="0.83029437251522864,0.83029437251522864" CornerSize="0.0848528137423857,0.0848528137423857"/>
                                    </local:GeometryProvider.Provider>
                                </GeometryModel3D>
                            </Model3DGroup>
                        </local:Model3DPresenter>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="mouseOverStoryboard">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="Material">
                                            <DiscreteObjectKeyFrame Value="{StaticResource mouseOverMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="BackMaterial">
                                            <DiscreteObjectKeyFrame Value="{StaticResource mouseOverMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="mouseOverStoryboard"/>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="pressedStoryboard">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="Material">
                                            <DiscreteObjectKeyFrame Value="{StaticResource clickMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="BackMaterial">
                                            <DiscreteObjectKeyFrame Value="{StaticResource clickMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="pressedStoryboard"/>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
