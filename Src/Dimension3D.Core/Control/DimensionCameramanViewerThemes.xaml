                        <ItemsPresenter/>
                        <local:Model3DPresenter x:Name="PART_MODEL" >
                            <Model3DGroup>
                                <GeometryModel3D x:Name="g" Material="{StaticResource vertexMaterial}" BackMaterial="{StaticResource vertexMaterial}">
                                    <local:GeometryProvider.Provider>
                                        <local:DimensionGeometryRegularPolygon Side="Front" Location="0,0,0"   Sides="6"  Radius="0.12" />
                                    </local:GeometryProvider.Provider>
                                    <GeometryModel3D.Transform>
                                        <RotateTransform3D>
                                            <RotateTransform3D.Rotation>
                                                <AxisAngleRotation3D Axis="0,0,1" Angle="30"/>
