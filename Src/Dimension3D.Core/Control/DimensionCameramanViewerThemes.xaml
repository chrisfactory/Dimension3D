                                                                                <TranslateTransform3D OffsetX="-0.5"/>
                                                                            </Transform3DGroup>
                                                                        </local:DimensionButton3D.Transform>
                                                                    </local:DimensionButton3D>
                                                                    <local:DimensionButton3D Style="{StaticResource EdgeButtonStyle}" Command="{x:Static local:DimensionCameramanViewer.FaceCommand}" CommandParameter="EdgeBackLeft">
