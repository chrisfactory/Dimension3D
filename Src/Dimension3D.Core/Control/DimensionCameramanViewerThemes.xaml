<ResourceDictionary 
    x:Class="Dimension3D.Core.DimensionCameramanViewerThemes"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dimension3D.Core">

    <DiffuseMaterial x:Key="faceMaterial" Brush="#9F57FFE8"/>
    <DiffuseMaterial x:Key="faceBackMaterial" Brush="Gray"/>
    <DiffuseMaterial x:Key="clickFaceMaterial" Brush="#FF9EFF57"/>
    <DiffuseMaterial x:Key="mouseOverFaceMaterial" Brush="#FFDF57FF"/>

    <Style x:Key="TopButtonStyle" TargetType="{x:Type local:DimensionButton3D}">
        <Setter Property="Template" >
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:DimensionButton3D}">
                    <Grid>
                        <ItemsPresenter/>
                        <local:Model3DPresenter x:Name="PART_MODEL" >
                            <Model3DGroup>
                                <Model3DGroup.Transform>
                                    <Transform3DGroup>
                                        <TranslateTransform3D OffsetY="0.185"/>
                                        <ScaleTransform3D x:Name="scale" ScaleX="1" ScaleY="1" ScaleZ="1"/>
                                    </Transform3DGroup>

                                </Model3DGroup.Transform>
                                <GeometryModel3D x:Name="g" Material="{StaticResource faceMaterial}" BackMaterial="{StaticResource faceBackMaterial}">
                                    <local:GeometryProvider.Provider>
                                        <local:DimensionGeometryPlan Side="Top" />
                                    </local:GeometryProvider.Provider>
                                </GeometryModel3D>
                            </Model3DGroup>
                        </local:Model3DPresenter>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="mouseOverStoryboard">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="Material">
                                            <DiscreteObjectKeyFrame Value="{StaticResource mouseOverFaceMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="BackMaterial">
                                            <DiscreteObjectKeyFrame Value="{StaticResource mouseOverFaceMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="scale" Storyboard.TargetProperty="ScaleX">
                                            <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1.2"/>
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="scale" Storyboard.TargetProperty="ScaleZ">
                                            <DiscreteDoubleKeyFrame KeyTime="0:0:0" Value="1.2"/>
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="mouseOverStoryboard"/>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="pressedStoryboard">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="Material">
                                            <DiscreteObjectKeyFrame Value="{StaticResource clickFaceMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames BeginTime="0:0:0" Duration="0:0:0" Storyboard.TargetName="g" Storyboard.TargetProperty="BackMaterial">
                                            <DiscreteObjectKeyFrame Value="{StaticResource clickFaceMaterial}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="pressedStoryboard"/>
                            </Trigger.ExitActions>
                        </Trigger> 
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
